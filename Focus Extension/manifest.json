{
"manifest_version": 2,

"name": "CodeAda Focus Extension",
"description": "This extension allows users to keep track of and focus on things",
"version": "1.0",

"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
},
"background": {
    "scripts": ["background.js"],
    "persistent": false
},
"permissions": [
    "activeTab", 
    "notifications",
    "alarms",
    "storage"
    ]
}